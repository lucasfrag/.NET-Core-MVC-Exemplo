@model Aluno

@{
    ViewData["Title"] = "Cadastro";
}
<h1>@ViewData["Title"] de alunos</h1>

<form method="POST">
    <p>
        <label asp-for="Nome">Nome: </label>
        <input asp-for="Nome" />
    </p>

    <p>
        <label asp-for="Idade">Idade: </label>
        <input asp-for="Idade" />
    </p>

    <p>
        <label asp-for="Nota">Nota: </label>
        <input asp-for="Nota" />
    </p>  

    <input type="submit" value="Cadastrar">
</form>